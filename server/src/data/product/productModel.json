{
	"type": "object",
	"additionalProperties": false,
	"required": [
		"ref",
		"name",
		"picture",
		"origin",
		"category",
		"wikiRef",
		"sale",
		"details",
		"supply",
		"manufacturing"
	],
	"properties": {
		"ref": { "type": "string" },
		"name": { "type": "string" },
		"detail": { "type": "string" },
		"picture": { "type": "string" },
		"origin": { "type": "string" },
		"category": {
			"type": "string",
			"enum": ["FRUIT", "VEGETABLE", "OTHER"]
		},
		"wikiRef": { "type": "string" },
		"sale": {
			"type": "object",
			"additionalProperties": false,
			"requiredProperties": [
				"priceHt",
				"minimumQuantity",
				"quantifier",
				"weight"
			],
			"properties": {
				"priceHt": { "type": "integer" },
				"promoPriceHt": { "type": "integer" },
				"minimumQuantity": { "type": "integer" },
				"quantifier": { "type": "string", "enum": ["UNIT", "KG"] },
				"weight": { "type": "integer" }
			}
		},
		"details": {
			"type": "object",
			"additionalProperties": false,
			"requiredProperties": ["isBio", "isLocal"],
			"properties": {
				"isBio": { "type": "boolean" },
				"isLocal": { "type": "boolean" }
			}
		},
		"supply": {
			"type": "object",
			"additionalProperties": false,
			"requiredProperties": ["price", "quantifier"],
			"properties": {
				"price": { "type": "integer" },
				"quantifier": { "type": "string", "enum": ["UNIT", "KG"] }
			}
		},
		"manufacturing": {
			"type": "object",
			"additionalProperties": false,
			"requiredProperties": [
				"quantifier",
				"supplier",
				"process",
				"lettring"
			],
			"properties": {
				"quantifier": { "type": "string", "enum": ["UNIT", "KG"] },
				"supplier": { "type": "string" },
				"process": { "type": "string" },
				"lettring": { "type": "string" }
			}
		}
	}
}
